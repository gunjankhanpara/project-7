<%- include('header')%>
        
        
        <!-- Content wrapper -->
    <div class="content-wrapper">
            <!-- Content -->
  
        <div class="container-xxl flex-grow-1 container-p-y">
              <h4 class="fw-bold py-3 mb-4"><span class="text-muted fw-light">BLOG</span> Vertical Layouts</h4>
  
              <!-- Basic Layout -->
            <div class="row">
                <div class="col-xl">
                    <div class="card mb-4">
                    <div class="card-header d-flex justify-content-between align-items-center">
                      <h5 class="mb-0">ADD Catagory</h5>
                      <small class="text-muted float-end">Default label</small>
                    </div>
                    <div class="card-body">
                        <form action="/profiledata" method="post">
                          <input type="text" name="editid" value="<%= locals.users.id %>">
                        <div class="mb-3">
                          <label class="form-label" for="basic-default-fullname">Name</label>
                          <input type="text" class="form-control" id="productname" name="name" value="<%= locals.users.name %>" placeholder="enter name" />
                        </div>
                        <div class="mb-3">
                          <label class="form-label" for="basic-default-company">Discription</label>
                          <input type="text" class="form-control" id="productprice" name="email" value="<%= locals.users.email %>" placeholder=" enter email" disabled/>
                        </div>
                        <div class="mb-3">
                            <label class="form-label" for="basic-default-company">Password</label>
                            <input type="text" class="form-control" id="productprice" name="password" value="<%= locals.users.password %>" placeholder=" enter password" />
                        </div>
                        <div class="mb-3">
                            <label class="form-label" for="basic-default-company">Confrim-Password</label>
                            <input type="text" class="form-control" id="productprice" name="cpassword" value="<%= locals.users.password %>" placeholder=" enter cpassword" />
                        </div>
                        <input type="submit" class="btn btn-primary" value="submit"></input>
                      </form>
                    </div>
                </div>  
            </div>
  
        </div>
    </div>
            <!-- / Content -->
  
  
  
            <div class="content-backdrop fade"></div>
          </div>
          <!-- Content wrapper -->


<%- include('footer')%>